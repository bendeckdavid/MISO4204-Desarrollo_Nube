							"    } else if (jsonData.status === 'pending') {",
							"        console.log('ðŸ“ Video is queued in SQS, waiting for worker');",
							"    } else if (jsonData.status === 'failed') {",
							"        console.log('âŒ Video processing failed - check DLQ');",
